<h1>SurveyResults#show</h1>
<p>Find me in app/views/survey_results/show.html.erb</p>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th>Description</th>
      <th>Battery Capacity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @survey_results.lighting_products.each do |lighting_product| %>
      <tr>
        <td><%= lighting_product.id %></td>
        <td><%= lighting_product.name %></td>
        <td><%= lighting_product.product_description %></td>
        <td><%= lighting_product.battery_capacity %></td>
        <td><%= link_to 'Show', lighting_product %></td>
        <td><%= link_to 'Edit', edit_lighting_product_path(lighting_product) %></td>
        <td><%= link_to 'Destroy', lighting_product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
